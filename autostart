#xfce-mcs-manager &

#Wallpaper
(sleep 1s && nitrogen --restore) &

#Compositor
(sleep 2s && compton) &

#Panel
(sleep 3s && tint2) &

#Dock
(sleep 4s && plank) &

#AppLauncher
(sleep 5s && rofi) &


